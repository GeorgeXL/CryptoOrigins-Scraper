{
  "functions": {
    "api/index.ts": {
      "maxDuration": 60
    },
    "api/[...slug].ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
